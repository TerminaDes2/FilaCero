{
  "name": "filacero-backend",
  "version": "0.3.0",
  "private": true,
  "description": "API FilaCero con Nest.js",
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "dev": "npx prisma generate && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --watch src --no-notify src/main.ts",
    "migrate:deploy": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "watch:prisma": "chokidar \"prisma/**/*.prisma\" -c \"npx prisma generate\""
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cron": "^2.0.1",
    "@types/nodemailer": "^7.0.3",
    "@types/sanitize-html": "^2.16.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "stripe": "^19.3.1",
    "twilio": "^4.23.0",
    "typeorm": "^0.3.20",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.9",
    "@types/bull": "^3.15.9",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.2"
  }
}
